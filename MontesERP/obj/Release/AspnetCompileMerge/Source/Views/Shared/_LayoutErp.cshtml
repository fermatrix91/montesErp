<body>

    <!-- Modal -->
    <div id="contieneModalDetalleSubCuenta" style="z-index: 10000;">
        <div id="modalSubCuenta" class="modal fade" role="dialog">
            <div class="modal-dialog">

                <!-- Modal content-->
                <div class="modal-content">
                    <div style="height: 45px;" class="modal-header">
                        <span style="float: left;" class="glyphicon glyphicon-wrench"></span>
                        <p style="float: left; font-weight: bold; margin-left: 5px;" id="tituloDetalleSubCuenta" class="modal-title"></p>
                        <button type="button" class="close" data-dismiss="modal" style="font-size: 30px; margin-top: -8px;">&times;</button>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-xs-12 col-sm-2 col-lg-2">
                                <p>Cuenta:</p>
                            </div>
                            <div class="col-xs-12 col-sm-10 col-lg-10">
                                <select class="form-control estiloInputs" id="dropDownCuentas"></select>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-12 col-sm-2 col-lg-2">
                                <p>Nombre:</p>
                            </div>
                            <div class="col-xs-12 col-sm-10 col-lg-10">
                                <input class="form-control estiloInputs" type="text" id="nombreSubCuenta" placeholder="Nombre de SubCuenta" />
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-12 col-sm-2 col-lg-2">
                                <p>Código:</p>
                            </div>
                            <div class="col-xs-12 col-sm-3 col-lg-3">
                                <p style="width: 100px;" id="inicioIdSubCuenta"></p>
                                <input style="display: none;" class="form-control estiloInputs" type="text" id="idSubCuenta" placeholder="Código" />
                            </div>
                        </div>

                    </div>
                    <div class="modal-footer">
                        <form id="formGuardaSubCuenta" action="/Contabilidad/GuardarSubCuenta" method="POST">
                            <button id="guardarSubCuenta" type="submit" class="btn btn-success" data-dismiss="modal"><span class="glyphicon glyphicon-check"></span></button>
                            <input id="idCuentaAEnviar" name="idCuenta" type="hidden" value="" />
                            <input id="idSubCuentaAEnviar" name="idSubCuenta" type="hidden" value="" />
                            <input id="nombreCuentaAEnviar" name="nombreCuenta" type="hidden" value="" />

                        </form>
                    </div>
                </div>

            </div>
        </div>
    </div>

    <!-- Modal -->
    <div id="contieneModalDetalleComprobante">
        <div id="modalComprobante" data-backdrop="static" class="modal fade" role="dialog">
            <div class="modal-dialog">

                <!-- Modal content-->
                <div class="modal-content">
                    <div style="height: 45px;" class="modal-header">
                        <span style="float: left;" class="glyphicon glyphicon-wrench"></span>
                        <p style="float: left; font-weight: bold; margin-left: 5px;" id="tituloDetalleComprobante" class="modal-title"></p>
                        <button type="button" class="close" data-dismiss="modal" style="font-size: 30px; margin-top: -8px;">&times;</button>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-xs-2 col-sm-2 col-lg-2">
                                <p>No.:</p>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-lg-2">
                                <p disabled="disabled" style="font-weight: bold;" id="numeroFolioComprobante"></p>
                            </div>

                            <div class="col-xs-3 col-sm-3 col-lg-3">
                                <p>Fecha Inicial:</p>
                            </div>
                            <div class="col-xs-5 col-sm-5 col-lg-5">
                                <input type="date" class="form-control estiloInputs" id="fechaInicialComprobante" />
                            </div>
                        </div>

                        <p></p>

                        <div class="row">
                            <div class="col-xs-12 col-sm-2 col-lg-2">
                                <p>Concepto:</p>
                            </div>
                            <div class="col-xs-12 col-sm-10 col-lg-10">
                                <input type="text" class="form-control estiloInputs" id="conceptoComprobante" placeholder="Concepto Aquí" />
                            </div>
                        </div>

                        <hr />

                        <div class="row">
                            <div class="col-xs-2 col-sm-2 col-lg-2">
                                <p>Cuenta:</p>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-lg-2">
                                <input id="numeroSubcuenta" style="width: 80px;" class="form-control estiloInputs" type="text" placeholder="#########" />
                            </div>
                            <div class="col-xs-8 col-sm-8 col-lg-8">
                                <select class="form-control estiloInputs" id="dropDownSubCuentas"></select>
                            </div>
                        </div>

                        <p></p>

                        <div class="row">
                            <div class="col-xs-2 col-sm-2 col-lg-2">
                                <p>Debe:</p>
                            </div>
                            <div class="col-xs-3 col-sm-3 col-lg-3">
                                <input class="form-control estiloInputs" type="text" id="debeComprobante" placeholder="0" />
                            </div>

                            <div class="col-xs-2 col-sm-2 col-lg-2">
                                <p>Haber:</p>
                            </div>
                            <div class="col-xs-3 col-sm-3 col-lg-3">
                                <input class="form-control estiloInputs" type="text" id="haberComprobante" placeholder="0" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-lg-2">
                                <button id="agregarMovimiento" style="float: right;" class="btn btn-info"><span class="glyphicon glyphicon-chevron-down"></span></button>
                            </div>
                        </div>

                        <p></p>

                        @*Movimientos*@
                        <div class="row">
                            <div style="max-height: 400px; overflow: auto;" id="divTablaDetalleComprobante">
                            </div>
                        </div>

                    </div>
                    <div class="modal-footer">
                        <form id="formGuardaComprobante" action="/Contabilidad/GuardarComprobante" method="POST">
                            <button id="guardarComprobante" type="submit" class="btn btn-success"><span class="glyphicon glyphicon-check"></span></button>
                            <input id="idCuentaAEnviar" name="idCuenta" type="hidden" value="" />
                            <input id="idSubCuentaAEnviar" name="idSubCuenta" type="hidden" value="" />
                            <input id="nombreCuentaAEnviar" name="nombreCuenta" type="hidden" value="" />

                        </form>
                    </div>
                </div>

            </div>
        </div>
    </div>


