@{
    ViewBag.Title = "_LayoutErp";
}

<!DOCTYPE html>
<html lang="es">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Montes - ERP</title>
    <script src="~/Scripts/jquery-1.12.3.min.js"></script>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/fonts/css/font-awesome.css" rel="stylesheet" />
    <link href="http://fonts.googleapis.com/css?family=Righteous" rel="stylesheet" type="text/css">
    <link href="~/Content/easy-autocomplete.themes.min.css" rel="stylesheet" />
    <link href="~/Content/easy-autocomplete.min.css" rel="stylesheet" />
    <link href="~/Content/DataTables-1.10.12/media/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/DataTables-1.10.12/extensions/Responsive/css/responsive.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/Menu/style.css" rel="stylesheet" />
</head>
<body>

    <!-- Modal -->
    <div id="modalSubCuenta" data-backdrop="static" class="modal fade" role="dialog">
        @Html.Partial("~/Views/Contabilidad/DetalleSubCuenta.cshtml");
    </div>

    <!-- Modal -->
    <div id="modalComprobante" data-backdrop="static" class="modal fade" role="dialog">
        @Html.Partial("~/Views/Contabilidad/DetalleComprobanteDeDiario.cshtml");
    </div>


    <header class="navbar">
        <div class="container-fluid expanded-panel">
            <div class="row">
                <div id="logo" class="col-xs-12 col-sm-2">
                    <a href="">Montes ERP</a>
                </div>
                <div id="top-panel" class="col-xs-12 col-sm-10">
                    <div class="row">
                        <div class="col-xs-8 col-sm-4">
                            <a href="#" class="show-sidebar">
                                <i class="fa fa-bars"></i>
                            </a>
                            <div id="search">
                                <input type="text" placeholder="search" />
                                <i class="fa fa-search"></i>
                            </div>
                        </div>
                        <div class="col-xs-4 col-sm-8 top-panel-right">
                            <ul class="nav navbar-nav pull-right panel-menu">
                                <li class="hidden-xs">
                                    <a href="" class="modal-link">
                                        <i class="fa fa-bell"></i>
                                        <span class="badge">7</span>
                                    </a>
                                </li>
                                <li class="hidden-xs">
                                    <a class="ajax-link" href="">
                                        <i class="fa fa-calendar"></i>
                                        <span class="badge">7</span>
                                    </a>
                                </li>
                                <li class="hidden-xs">
                                    <a href="" class="ajax-link">
                                        <i class="fa fa-envelope"></i>
                                        <span class="badge">7</span>
                                    </a>
                                </li>
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle account" data-toggle="dropdown">
                                        <div class="avatar">
                                            <img src="img/avatar.jpg" class="img-rounded" alt="avatar" />
                                        </div>
                                        <i class="fa fa-angle-down pull-right"></i>
                                        <div class="user-mini pull-right">
                                            <span class="welcome">Welcome,</span>
                                            <span>Jane Devoops</span>
                                        </div>
                                    </a>
                                    <ul class="dropdown-menu">
                                        <li>
                                            <a href="#">
                                                <i class="fa fa-user"></i>
                                                <span class="hidden-sm text">Profile</span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="" class="ajax-link">
                                                <i class="fa fa-envelope"></i>
                                                <span class="hidden-sm text">Messages</span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="" class="ajax-link">
                                                <i class="fa fa-picture-o"></i>
                                                <span class="hidden-sm text">Albums</span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="" class="ajax-link">
                                                <i class="fa fa-tasks"></i>
                                                <span class="hidden-sm text">Tasks</span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <i class="fa fa-cog"></i>
                                                <span class="hidden-sm text">Settings</span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <i class="fa fa-power-off"></i>
                                                <span class="hidden-sm text">Logout</span>
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!--End Header-->
    <!--Start Container-->
    <div id="main" class="container-fluid">
        <div class="row">
            <div id="sidebar-left" class="col-xs-2 col-sm-2">
                <ul class="nav main-menu">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle">
                            <i class="fa fa-calculator"></i>
                            <span class="hidden-xs">Contabilidad</span>
                        </a>
                        <ul class="dropdown-menu">
                            <li><a class="ajax-link" href="/Contabilidad/CatalogoDeCuentas">Catálogo de Cuentas</a></li>
                            <li><a class="ajax-link" href="/Contabilidad/ComprobantesDeDiario">Comprobantes de Diario</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle">
                            <i class="fa fa-newspaper-o"></i>
                            <span class="hidden-xs">Facturación</span>
                        </a>
                        <ul class="dropdown-menu">
                            <li><a class="ajax-link" href="/Contabilidad/CatalogoDeCuentas">Compras</a></li>
                            <li><a class="ajax-link" href="/Contabilidad/ComprobantesDeDiario">Ventas</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
            <!--Start Content-->
            <div id="content" class="col-xs-12 col-sm-10">
                @RenderBody()
            </div>
            <!--End Content-->
        </div>
    </div>
    <!--End Container-->
    @RenderSection("scripts", required: false)

    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
    <script src="~/Scripts/jquery.blockUI.js"></script>
    <script src="~/Scripts/jquery.easy-autocomplete.min.js"></script>
    <script src="~/Scripts/DataTables-1.10.12/media/js/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/DataTables-1.10.12/extensions/Responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/Scripts/arrayToTable.js"></script>
    <script src="~/Scripts/Menu/devoops.js"></script>
</body>
</html>